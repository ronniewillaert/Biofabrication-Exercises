        "# Interactive bioink selection tool\n",
        "\n",
        "def find_optimal_bioink(requirements, weights=None, min_thresholds=None):\n",
        "    \"\"\"\n",
        "    Find the best bioink based on specific requirements.\n",
        "    \n",
        "    Parameters:\n",
        "    requirements: dict of desired property levels\n",
        "    weights: importance weights for each category\n",
        "    min_thresholds: minimum acceptable scores for critical properties\n",
        "    \n",
        "    Returns:\n",
        "    ranked bioinks with suitability scores\n",
        "    \"\"\"\n",
        "    \n",
        "    if weights is None:\n",
        "        weights = {cat: 1.0 for cat in property_categories.keys()}\n",
        "    \n",
        "    if min_thresholds is None:\n",
        "        min_thresholds = {}\n",
        "    \n",
        "    # Calculate suitability scores\n",
        "    suitability_scores = {}\n",
        "    detailed_scores = {}\n",
        "    \n",
        "    for bioink in bioink_names:\n",
        "        total_score = 0\n",
        "        total_weight = 0\n",
        "        bioink_details = {}\n",
        "        passes_threshold = True\n",
        "        \n",
        "        for category, properties in property_categories.items():\n",
        "            if category in requirements:\n",
        "                # Calculate category score\n",
        "                category_score = bioink_df.loc[bioink, properties].mean()\n",
        "                \n",
        "                # Check minimum threshold\n",
        "                if category in min_thresholds:\n",
        "                    if category_score < min_thresholds[category]:\n",
        "                        passes_threshold = False\n",
        "                \n",
        "                # Weight by importance and distance to requirement\n",
        "                target = requirements[category]\n",
        "                distance_penalty = abs(category_score - target) / 10  # Normalize\n",
        "                adjusted_score = category_score * (1 - distance_penalty)\n",
        "                \n",
        "                weight = weights.get(category, 1.0)\n",
        "                total_score += adjusted_score * weight\n",
        "                total_weight += weight\n",
        "                \n",
        "                bioink_details[category] = {\n",
        "                    'score': category_score,\n",
        "                    'target': target,\n",
        "                    'distance': abs(category_score - target),\n",
        "                    'adjusted_score': adjusted_score\n",
        "                }\n",
        "        \n",
        "        if total_weight > 0 and passes_threshold:\n",
        "            suitability_scores[bioink] = total_score / total_weight\n",
        "            detailed_scores[bioink] = bioink_details\n",
        "        else:\n",
        "            suitability_scores[bioink] = 0  # Failed threshold\n",
        "            detailed_scores[bioink] = bioink_details\n",
        "    \n",
        "    return suitability_scores, detailed_scores\n",
        "\n",
        "# Example application scenarios\n",
        "scenarios = {\n",
        "    'High_Precision_Implant': {\n",
        "        'requirements': {'Mechanical': 8, 'Biological': 7, 'Processing': 7},\n",
        "        'weights': {'Mechanical': 0.4, 'Biological': 0.4, 'Processing': 0.2},\n",
