        "    \n",
        "    suitability, details = find_optimal_bioink(\n",
        "        scenario_data['requirements'],\n",
        "        scenario_data['weights'],\n",
        "        scenario_data['min_thresholds']\n",
        "    )\n",
        "    \n",
        "    # Display requirements\n",
        "    req_str = ', '.join([f'{k}‚â•{v}' for k, v in scenario_data['requirements'].items()])\n",
        "    print(f\"   Requirements: {req_str}\")\n",
        "    \n",
        "    # Display thresholds\n",
        "    if scenario_data['min_thresholds']:\n",
        "        thresh_str = ', '.join([f'{k}‚â•{v}' for k, v in scenario_data['min_thresholds'].items()])\n",
        "        print(f\"   Minimum thresholds: {thresh_str}\")\n",
        "    \n",
        "    # Rank and display results\n",
        "    ranked = sorted(suitability.items(), key=lambda x: x[1], reverse=True)\n",
        "    print(\"   Recommendations:\")\n",
        "    \n",
        "    for i, (bioink, score) in enumerate(ranked[:3], 1):\n",
        "        if score > 0:\n",
        "            print(f\"     {i}. {bioink.replace('_', ' ')}: {score:.2f}/10\")\n",
        "            \n",
        "            # Show key strengths\n",
        "            bioink_detail = details[bioink]\n",
        "            strengths = []\n",
        "            for cat, data in bioink_detail.items():\n",
        "                if data['distance'] < 1.0:  # Close to target\n",
        "                    strengths.append(f\"{cat} ({data['score']:.1f})\")\n",
        "            \n",
        "            if strengths:\n",
        "                print(f\"        Strengths: {', '.join(strengths)}\")\n",
        "        else:\n",
        "            print(f\"     {i}. {bioink.replace('_', ' ')}: Failed minimum thresholds\")\n",
        "\n",
        "# Create visualization of optimization results\n",
        "fig, axes = plt.subplots(2, 2, figsize=(16, 12))\n",
        "fig.suptitle('Bioink Selection Optimization Results', fontsize=16, fontweight='bold')\n",
        "\n",
        "for i, (scenario_name, scenario_data) in enumerate(list(scenarios.items())[:4]):\n",
        "    ax = axes[i//2, i%2]\n",
        "    \n",
