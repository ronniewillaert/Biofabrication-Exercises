        "# Extension Challenge: Custom bioink designer\n",
        "\n",
        "def design_custom_bioink(base_bioink, modifications, target_application):\n",
        "    \"\"\"\n",
        "    Design a custom bioink by modifying an existing one.\n",
        "    \n",
        "    Parameters:\n",
        "    base_bioink: starting bioink type\n",
        "    modifications: dict of property changes (+/- values)\n",
        "    target_application: intended use case\n",
        "    \n",
        "    Returns:\n",
        "    modified bioink properties and performance analysis\n",
        "    \"\"\"\n",
        "    \n",
        "    # Start with base bioink properties\n",
        "    custom_properties = bioink_df.loc[base_bioink].copy()\n",
        "    \n",
        "    # Apply modifications\n",
        "    for property_name, change in modifications.items():\n",
        "        if property_name in custom_properties.index:\n",
        "            new_value = custom_properties[property_name] + change\n",
        "            custom_properties[property_name] = max(1, min(10, new_value))  # Clamp to 1-10\n",
        "    \n",
        "    return custom_properties\n",
        "\n",
        "def evaluate_custom_bioink(custom_properties, target_weights):\n",
        "    \"\"\"\n",
        "    Evaluate how well a custom bioink meets target requirements.\n",
        "    \"\"\"\n",
        "    \n",
        "    # Calculate category scores\n",
        "    category_scores = {}\n",
        "    for category, properties in property_categories.items():\n",
        "        category_scores[category] = custom_properties[properties].mean()\n",
        "    \n",
        "    # Calculate weighted total\n",
        "    total_score = sum(category_scores[cat] * weight for cat, weight in target_weights.items())\n",
        "    \n",
        "    return category_scores, total_score\n",
